;;; -*- Lisp -*-

(in-package :asdf)

(assert (handler-case
            (let ((*compile-file-failure-behaviour* :warn))
              (load-system 'test-compile-file-failure :force t)
              t)
          (compile-file-error () nil)))

(assert (handler-case
            (let ((*compile-file-failure-behaviour* :error))
              (load-system 'test-compile-file-failure :force t)
              nil)
          (compile-file-error () t)))
